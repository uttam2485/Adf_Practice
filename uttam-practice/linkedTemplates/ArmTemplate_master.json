{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "uttam-practice"
		},
		"AzureBlobStorage1_uttam_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1_uttam'"
		},
		"AzureSqlDatabase2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase2'"
		},
		"LS_SQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLServer'"
		},
		"SqlServer_linkedServices_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer_linkedServices'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adf.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Azure_sql_database"
		},
		"AzureSqlDatabase2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adf.database.windows.net"
		},
		"AzureSqlDatabase2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Azure_sql_database"
		},
		"AzureSqlDatabase2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "uttam"
		},
		"LS_SQLServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-HS5G5B7"
		},
		"LS_SQLServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ADF-Praactice"
		},
		"LS_SQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "uttam"
		},
		"SqlServer_linkedServices_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-HS5G5B7"
		},
		"SqlServer_linkedServices_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ADF-Praactice"
		},
		"SqlServer_linkedServices_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Uttam"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "uttam-practice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_uttam_connectionString": {
						"value": "[parameters('AzureBlobStorage1_uttam_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "uttam-practice_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase2_password": {
						"value": "[parameters('AzureSqlDatabase2_password')]"
					},
					"LS_SQLServer_password": {
						"value": "[parameters('LS_SQLServer_password')]"
					},
					"SqlServer_linkedServices_password": {
						"value": "[parameters('SqlServer_linkedServices_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_userName')]"
					},
					"LS_SQLServer_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_server')]"
					},
					"LS_SQLServer_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_database')]"
					},
					"LS_SQLServer_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_userName')]"
					},
					"SqlServer_linkedServices_properties_typeProperties_server": {
						"value": "[parameters('SqlServer_linkedServices_properties_typeProperties_server')]"
					},
					"SqlServer_linkedServices_properties_typeProperties_database": {
						"value": "[parameters('SqlServer_linkedServices_properties_typeProperties_database')]"
					},
					"SqlServer_linkedServices_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer_linkedServices_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'uttam-practice_ArmTemplate_0')]"
			]
		},
		{
			"name": "uttam-practice_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'uttam-practice_ArmTemplate_1')]"
			]
		}
	]
}